{
    "name": "room/letexto-sms-package",
    "description": "Package PHP pour l'envoi de SMS via l'API Letexto",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DA Sie Roger",
            "email": "dsieroger@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Room\\Sms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Room\\Sms\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
